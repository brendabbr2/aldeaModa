<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index</title>
</head>
<body>
    <h1>Usuarios</h1>
    <ul>
        <% usuarios.forEach(function(usuario) { %>
            <li>
                <strong>ID:</strong> <%= usuario.usuarioId %><br>
                <strong>Nombre:</strong> <%= usuario.nombre %><br>
                <strong>Apellido 1:</strong> <%= usuario.apellido1 %><br>
                <strong>Apellido 2:</strong> <%= usuario.apellido2 %><br>
                <strong>Correo:</strong> <%= usuario.correo %><br>
                <strong>Dirección:</strong> <%= usuario.direccion %><br>
                <strong>Ciudad:</strong> <%= usuario.ciudad %><br>
                <strong>País:</strong> <%= usuario.pais %><br>
            </li>
        <% }); %>
    </ul>
</body>
</html>
