	<!-- Product -->
	<section class="bg0 p-t-23 p-b-140">
		<div class="container">
			<div class="p-b-10">
				<h3 class="ltext-103 cl5">
					Product Overview
				</h3>
			</div>

			<div class="flex-w flex-sb-m p-b-52">
				<div class="flex-w flex-l-m filter-tope-group m-tb-10">
					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1" data-filter="*">
						All Products
					</button>

					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter=".women">
						Women
					</button>

					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter=".men">
						Men
					</button>

					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter=".girl">
						Girl
					</button>

					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5" data-filter=".boy">
						Boy
					</button>
				</div>				
			</div>

			<div class="row isotope-grid">
	
				<!--Empieza foreach-->
			
				<% products.forEach(function(product) { %>
					<div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item <%= product.category_name.toLowerCase() %>">
						<div class="block2">
							<div class="block2-pic hov-img0">
								<img src="static/e-commerse/images/<%= product.image_name %>" alt="<%= product.name %>">
				
								<a href="shoping-cart.html?productId=<%=product.id%>&name=<%=product.name%>&price=<%=product.price%>&imageName=<%=product.image_name%>" class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04">
									Buy!
								</a>
							</div>
				
							<div class="block2-txt flex-w flex-t p-t-14">
								<div class="block2-txt-child1 flex-col-l">
									<span class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
										<%= product.name %>
									</span>
				
									<span class="stext-105 cl3">
										$<%= product.price %>
									</span>
								</div>
							</div>
						</div>
					</div>
				<% }); %>      
			
					<!--Termina foreach-->
			</div>

		</div>
	</section>


